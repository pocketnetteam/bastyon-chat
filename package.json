{
  "name": "vue-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node themes.js && vue-cli-service build --dest /run/media/freedom/Server/nginx/hosts/localhost/pocketnet/chat/",
    "build_mac": "node themes.js && vue-cli-service build --dest ~/work/pocketnet/chat",
    "themes": "node themes.js",
    "buildpocketnet": "npm run build -- --target wc-async --inline-vue --name matrix-element src/App.vue",
    "buildpocketnet_mac": "npm run build_mac -- --target wc-async --inline-vue --name matrix-element src/App.vue",
    "buildpocketnetdev": "npm run build -- --target wc-async --inline-vue --watch --name matrix-element src/App.vue",
    "prepare": "husky install",
    "lint-staged": {
      "**/*.{js,vue}": "prettier --write --ignore-unknown"
    },
    "lint-check": "yarn prettier --check './src/**/*.{js,vue}'"
  },
  "dependencies": {
